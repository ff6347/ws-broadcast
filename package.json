  },
  "bin": {
    "ws-broadcast": "./lib/cli.js"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-unobtrusive": "^1.2.2",
